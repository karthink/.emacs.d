# -*- mode: snippet -*-
# name: search
# key: search
# --
(progn
  (set-buffer "${1:`(buffer-name (other-buffer))`}")
  (mapc 
   (lambda (x) 
     (save-excursion 
       (while (search-forward (car x) nil t)
         (replace-match (cdr x) nil t))))
   '(("$2" . "$3")
     ("$4" . "$5")$0
     )))
